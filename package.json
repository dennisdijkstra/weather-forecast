{
  "name": "technical-assignment-front-end-engineer-dennisdijkstra",
  "version": "1.0.0",
  "description": "Make an application that allows the user to view the observed (in the past 30 days) or forecasted (in the future) daily weather conditions for a given location using the [Dark Sky API](https://darksky.net/dev/docs).",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack-dev-server --mode development ./client/src/client.js --open",
    "dev": "webpack --mode development ./client/src/client.js --output ./client/dist/bundle.js --open",
    "build": "webpack --mode production ./client/src/client.js --output ./client/dist/bundle.js --open",
    "start": "concurrently \"npm:dev\" \"npm:webpack\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madewithlove/technical-assignment-front-end-engineer-dennisdijkstra.git"
